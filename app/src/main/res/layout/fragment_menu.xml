<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B6B6B6"
    android:visibility="visible">

    <LinearLayout
        android:id="@+id/categoryView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        android:orientation="vertical"
        android:paddingTop="10dp"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/linearLayout_projectCat"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="28dp"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:weightSum="10">

            <ImageView
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.2"
                android:padding="12dp"
                android:src="@drawable/icon_project" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="프로젝트"
                android:textColor="#000000"
                android:textSize="16sp" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayout_portfolioCat"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:weightSum="10">

            <ImageView
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.1"
                android:padding="12dp"
                android:src="@drawable/icon_portfolio" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="9.7"
                android:text="포트폴리오"
                android:textColor="#000000"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/unfold01"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.2"
                android:padding="12dp"
                android:src="@drawable/icon_unfold" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayout_portfolioDetail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.5dp"
                android:background="#E7E7E7"
                android:paddingLeft="58dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="프로젝트 우승작"
                android:textColor="#3E3E3E"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.5dp"
                android:background="#E7E7E7"
                android:paddingLeft="58dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="개인 포트폴리오"
                android:textColor="#3E3E3E"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout_trendCat"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:weightSum="10">

            <ImageView
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.1"
                android:padding="12dp"
                android:src="@drawable/icon_trend" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="9.7"
                android:text="트렌드"
                android:textColor="#000000"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/unfold02"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.2"
                android:padding="12dp"
                android:src="@drawable/icon_unfold" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayout_trendDetail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.5dp"
                android:background="#E7E7E7"
                android:paddingLeft="58dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="개발 트렌드"
                android:textColor="#3E3E3E"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.5dp"
                android:background="#E7E7E7"
                android:paddingLeft="58dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="디자인 트렌드"
                android:textColor="#3E3E3E"
                android:textSize="15sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout_allianceCat"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:weightSum="10">

            <ImageView
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.2"
                android:padding="12dp"
                android:src="@drawable/icon_alliance" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="제휴서비스"
                android:textColor="#000000"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout_serviceCat"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:weightSum="10">

            <ImageView
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.1"
                android:padding="12dp"
                android:src="@drawable/icon_service" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="9.7"
                android:text="고객센터"
                android:textColor="#000000"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/unfold03"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="0.2"
                android:padding="12dp"
                android:src="@drawable/icon_unfold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout_serviceDetail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.5dp"
                android:background="#E7E7E7"
                android:paddingLeft="58dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="공지사항"
                android:textColor="#3E3E3E"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.5dp"
                android:background="#E7E7E7"
                android:paddingLeft="58dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="FAQ"
                android:textColor="#3E3E3E"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.5dp"
                android:background="#E7E7E7"
                android:paddingLeft="58dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="문의하기"
                android:textColor="#3E3E3E"
                android:textSize="15sp" />

        </LinearLayout>


    </LinearLayout>

    <ScrollView
        android:id="@+id/projectIngView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:fillViewport="true"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView_menutitle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="프로젝트"
                android:textColor="#000000"
                android:textSize="24sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/logo01" />

                <EditText
                    android:id="@+id/editTextTextPersonName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:hint="검색어를 입력하세요"
                    android:inputType="textPersonName" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView_progress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="진행중"
                        android:textColor="#000000"
                        android:textSize="18sp" />

                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView_judge"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="심사중"
                        android:textColor="#000000"
                        android:textSize="18sp" />

                    <View
                        android:id="@+id/view3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView_end"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="종료"
                        android:textColor="#000000"
                        android:textSize="18sp" />

                    <View
                        android:id="@+id/view4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </LinearLayout>
            </LinearLayout>

            <ListView
                android:id="@+id/listView_projectList"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:orientation="vertical">

            </LinearLayout>

        </LinearLayout>
    </ScrollView>




</FrameLayout>


